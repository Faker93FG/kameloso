{
    "name": "kameloso",
    "description": "IRC bot in D",
    "authors": [ "JR" ],
    "copyright": "Copyright Â© 2018, JR",
    "license": "MIT",
    "targetType": "executable",
    "mainSourceFile": "source/kameloso/main.d",
    "sourcePaths": [ "source" ],
    "importPaths": [ "source" ],
    "libs-posix-dmd": [ "phobos2" ],
    "-ddoxFilterArgs":
    [
        "--unittest-examples",
        "--ex", "arsd."
    ],
    "-ddoxTool" : "scod",
    "versions":
    [
        "AsAnApplication",
        "TwitchSupport",
        "WithAdminPlugin",
        "WithAutomodePlugin",
        "WithBashQuotesPlugin",
        "WithChanQueriesService",
        "WithChatbotPlugin",
        "WithConnectService",
        "WithCTCPService",
        "WithHelpPlugin",
        "WithNotesPlugin",
        "WithPersistenceService",
        "WithPipelinePlugin",
        "WithPrinterPlugin",
        "WithQuotesPlugin",
        "WithRedditPlugin",
        "WithSedReplacePlugin",
        "WithSeenPlugin",
        "WithWebtitlesPlugin"
    ],
    "configurations":
    [
        {
            "name": "posix",
            "platforms": [ "posix" ],
            "versions": [ "WithPlugins", "Colours", "Web" ],
            "dependencies": { "requests": "~>1.0.0" }
        },
        {
            "name": "windows",
            "platforms": [ "windows" ],
            "versions": [ "WithPlugins", "Colours", "Web" ],
            "dependencies": { "requests": "~>1.0.0" }
        },
        {
            "name": "twitch",
            "versions": [ "WithPlugins", "Colours", "Web", "WithTwitchBotPlugin" ],
            "dependencies": { "requests": "~>1.0.0" }
        },
        {
            "name": "polyglot",
            "versions": [ "WithPlugins", "Colours", "Web",
                "WithTwitchBotPlugin", "TwitchWarnings", "OmniscientAdmin", "PrintStacktraces" ],
            "dependencies": { "requests": "~>1.0.0" }
        },
        {
            "name": "vanilla",
            "versions": [ "WithPlugins" ]
        },
        {
            "name": "colours",
            "versions": [ "WithPlugins", "Colours" ]
        },
        {
            "name": "web",
            "versions": [ "WithPlugins", "Web" ],
            "dependencies": { "requests": "~>1.0.0" }
        },
        {
            "name": "full",
            "versions": [ "WithPlugins", "Colours", "Web" ],
            "dependencies": { "requests": "~>1.0.0" }
        },
        {
            "name": "pluginless",
            "versions": [ "Colours" ]
        },
        {
            "name": "pluginless-vanilla"
        },

        {
            "name": "unittest",
            "buildType": "unittest",
            "versions": [ "WithPlugins", "Web", "Colours", "TwitchWarnings",
                "WithTwitchBotPlugin", "OmniscientAdmin", "PrintStacktraces", "ExplainReplay" ],
            "sourcePaths": [ "tests" ],
            "importPaths": [ "tests" ],
            "dflags": [ "-dip25", "-dip1000", "-dip1008" ],
            "dependencies": { "requests": "~>1.0.0" }
        }
    ]
}
